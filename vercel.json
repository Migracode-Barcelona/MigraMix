{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|css|png|jpg|svg|gif|json))",
      "dest": "/src/$1"
    },
    {
      "src": "/(.*\\.(html|ico))",
      "dest": "/src/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
